<p>
    <strong>Title:</strong>
    <%= @test.title %>
</p>

<p>
    <strong>Level:</strong>
    <%= @test.level %>
</p>

<p>
    <strong>Creator:</strong>
    <%= @test.creator.email %>
</p>

<p>
    <strong>Category:</strong>
    <%= @test.category.name %>
</p>

<h3>Questions: </h3>

<% @test.questions.each do |question| %>
    <p> <%= question.title %></p>
    <%= link_to 'Open', question_path(question.id)%>
<% end %>

<%= link_to 'Back', tests_path %>
