
        <tr>
            <td><%= test.title %></td>
            <td><%= test_level(test) %></td>
            <td class="questions-quantity"><%= test.questions.length %></td>
            <td>
              <% if test.questions.length > 0 %>
                <%= button_to t('.start'), start_test_path(test), form_class: "start-test" %>
              <% else %>
                <%= button_to t('.start'), start_test_path(test), form_class: "start-test", disabled: true %>
              <% end %>
            </td>
        </tr>
