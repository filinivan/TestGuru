<%= form_with model: [@test, @question], local: true do |form| %>
  
<%= render 'shared/errors', resource: @question %>
  
  <p>
    <%= form.label :title %>
    <%= form.text_field :title, id: :question_title%>
  </p>

  <p>
    <%= form.label :user_id %>
    <%= form.number_field :user_id %>
  </p>

  <p> <%= form.submit %> </p>
<% end %>
